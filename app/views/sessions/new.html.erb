<h2>Log In</h2>

<form action="<%= session_url %>" method="post">
  <%= authenticity_token %>

  <label>Username:
    <input type="text" name="session[username]" value="<%= @user.username %>">
  </label><br>

  <label>Password:
    <input type="password" name="session[password]">
  </label><br>

  <button>Log In</button>
</form>